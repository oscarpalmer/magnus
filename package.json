{
  "author": {
    "name": "Oscar Palm√©r",
    "url": "https://oscarpalmer.se"
  },
  "description": "A JavaScript framework for developers who like HTML.",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bun": "^1.1.6",
    "bun": "^1.1.20",
    "typescript": "^5.5.3"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/magnus.mjs",
      "require": "./dist/magnus.js"
    }
  },
  "jsdelivr": "dist/magnus.iife.js",
  "keywords": [
    "framework",
    "frontend",
    "progressive enhancement",
    "stimulusjs"
  ],
  "license": "MIT",
  "main": "dist/magnus.iife.js",
  "module": "dist/magnus.esm.js",
  "name": "magnus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscarpalmer/magnus.git"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:iife",
    "build:esm": "esbuild ./src/index.ts --bundle --target=es2020 --platform=browser --outfile=./dist/magnus.esm.js --format=esm",
    "build:iife": "esbuild ./src/index.ts --bundle --target=es2020 --platform=browser --outfile=./dist/magnus.iife.js --global-name=Magnus --format=iife",
    "watch": ""
  },
  "types": "src/types.d.ts",
  "unpkg": "dist/magnus.iife.js",
  "version": "0.30.0"
}
