		"jsdom": "^27.4",
		"oxfmt": "^0.23",
		"oxlint": "^1.38",
		"rolldown": "1.0.0-beta.58",
		"tslib": "^2.8",
		"typescript": "^5.9",
		"vite": "8.0.0-beta.5",
